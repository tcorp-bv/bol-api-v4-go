{
	"name": "Docker",
	"dockerFile": "Dockerfile",
	"runArgs": [ "--privileged" ],
	"settings": { 
		"terminal.integrated.shell.linux": "/bin/bash",
		"go.gopath": "~/go",
		"go.inferGopath": true
	},
	"extensions": [
		"golang.go"
	],
	"postCreateCommand": "dockerd-entrypoint.sh --experimental &",

	"remoteUser": "rootless"
}
